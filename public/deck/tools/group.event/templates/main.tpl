<script id="event-tmpl-main" type="text/template">
  <%
    /**
     *  Event Main Template
     *
     *  @author     Saneyuki Tadokoro <post@saneyuki.gfunction.com>
     *  @copyright  2012, +place
    */
  %>
  <div class="ui-container event-container">

    <div class="ui-header">
      <div class="ui-header-image">
        <img src="tools/group.event/style/images/icon.png" alt="" />
      </div>
      <div class="ui-header-text">
        <span>イベント</span>
      </div>
    </div>

    <div class="ui-body">
      <div class="ui-box event-box">
        <div class="ui-inner-box event-inner-box-join">
          <span>参加しているイベント</span>
        </div>
      </div>
      <div class="ui-box event-box">
        <div class="ui-inner-box event-inner-box-contents">
        </div>
      </div>

      <div class="ui-box event-box">
        <div class="ui-inner-box event-inner-box-hold">
          <span>管理しているイベント</span>
        </div>
      </div>
      <div class="ui-box event-box">
        <div class="ui-inner-box event-inner-box-contents">
        </div>
      </div>
    </div>

  </div>
</script>
